    Range("G25").Select
    ActiveCell.FormulaR1C1 = "=RC[-4]^2"
    Range("G26").Select
    ActiveCell.FormulaR1C1 = "=RC[-4]^2"
    Range("G27").Select
    ActiveCell.FormulaR1C1 = "=RC[-4]^2"
    Range("G28").Select
    ActiveCell.FormulaR1C1 = "=RC[-4]^2"
    Range("G29").Select
    ActiveCell.FormulaR1C1 = "=RC[-4]^2"
    Range("G30").Select
    ActiveCell.FormulaR1C1 = "=RC[-4]^2"
    Range("G31").Select
    ActiveCell.FormulaR1C1 = "=RC[-4]^2"
    Range("G32").Select
    ActiveCell.FormulaR1C1 = "=RC[-4]^2"
    Range("G33").Select
    ActiveCell.FormulaR1C1 = "=RC[-4]^2"
    Range("G34").Select
    ActiveCell.FormulaR1C1 = "=RC[-4]^2"
    Range("H24").Select
    ActiveCell.FormulaR1C1 = ""
    Range("F14").Select
End Sub

Sub Magnitude()
'
' Magnitude Macro
'

'
    Range("I24").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I25").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I26").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I27").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I28").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I29").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I30").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I31").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I32").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I33").Select
    ActiveCell.Formula2R1C1 = "=sqrt"
    Range("I33").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I34").Select
    ActiveCell.FormulaR1C1 = "=SQRT(RC[-2]+RC[-1])"
    Range("I35").Select
End Sub
Sub Polar_Radian()
'
' Polar Macro
'

'
    Range("K24").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K25").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K26").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K27").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K28").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K29").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K30").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K31").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K32").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K33").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K34").Select
    ActiveCell.FormulaR1C1 = "=ATAN(RC[-7]/RC[-8])"
    Range("K35").Select
End Sub

Sub Polar_Degree()
'
' Polar_Degree Macro
' Converts Radians into Degrees
'

'
    Range("A38").Select
    ActiveCell.Formula2R1C1 = "=DEGREES(R[-14]C[10]:R[-4]C[10])"
    Range("B46").Select
End Sub

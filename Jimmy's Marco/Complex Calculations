Sub Making_Complex()
'
' Making_Complex Macro
'

'
    ActiveCell.FormulaR1C1 = "=COMPLEX(5,5)"
    Range("A25").Select
    ActiveCell.FormulaR1C1 = "=COMPLEX(4,-5)"
    Range("A26").Select
    ActiveCell.FormulaR1C1 = "=COMPLEX(15,-6)"
    Range("A27").Select
    ActiveCell.FormulaR1C1 = "=COMPLEX(-8,-9)"
    Range("A28").Select
    ActiveCell.FormulaR1C1 = "=COMPLEX(-6,7)"
    Range("A29").Select
    ActiveCell.FormulaR1C1 = "=COMPLEX(3,-9)"
    Range("A30").Select
    ActiveCell.FormulaR1C1 = "=COMPLEX(12.2,-4.5)"
    Range("A31").Select
    ActiveCell.FormulaR1C1 = "=COMPLEX(-45,35)"
    Range("A32").Select
    ActiveCell.FormulaR1C1 = "=COMPLEX(6,-15)"
    Range("A33").Select
    ActiveCell.FormulaR1C1 = "=COMPLEX(3,-3)"
    Range("A34").Select
    ActiveCell.FormulaR1C1 = "=COMPLEX(2.3,-1.2)"
    Range("A35").Select
End Sub
Sub Real_Number_Part()
'
' Real_Number_Part Macro
'

'
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C25").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C26").Select
    ActiveCell.Formula2R1C1 = "=Imrea"
    Range("C26").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C27").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C28").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C29").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C30").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C31").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C32").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C33").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C34").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C25").Select
    ActiveCell.FormulaR1C1 = ""
    Range("C25").Select
    ActiveCell.FormulaR1C1 = "=IMREAL(RC[-2])"
    Range("C26").Select
End Sub
Sub Imaginary_Number_Part()
'
' Imaginary_Number_Part Macro
'
'
Dim rng As Range
Set rng = Range("D24")
rng.Name = "Imaginary"


    Range("D24").Select
    ActiveCell.Formula2R1C1 = "=Im"
    Range("D24").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D25").Select
    ActiveCell.Formula2R1C1 = "=IMA"
    Range("D25").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D26").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D27").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D28").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D29").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D30").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D31").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D32").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D33").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D34").Select
    ActiveCell.FormulaR1C1 = "=IMAGINARY(RC[-3])"
    Range("D35").Select
End Sub
Sub Real_Number_Squared()
'
' Real_Number_Squared Macro
'

'
